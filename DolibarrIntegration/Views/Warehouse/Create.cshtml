@{
    ViewData["Title"] = "Editar o Crear";
}
<h1>@ViewData["Title"]</h1>



@model DolibarrIntegration.Dto.Warehouses


<hr />


<form asp-action="CreateWare" asp-controller="Warehouse" formmethod="post" enctype="multipart/form-data">
   
    
    <div class="mb-3">
        <label for="Address" class="form-label">Referencia</label>
        <div class="col-md-4">
            @Html.TextBoxFor(m => m.label, new { @class = "form-control", @placeholder = "EnterName", @requried = "required" })
        </div>

    </div>

    <div class="mb-3">
        <label for="Cellnumber" class="form-label">Nombre</label>
        <div class="col-md-4">
            @Html.TextBoxFor(m => m.lieu, new { @class = "form-control", @placeholder = "EnterName", @requried = "required" })
        </div>

    </div>
    <div class="mb-3">
        <label for="Emailid" class="form-label">Descripcion</label>
        <div class="col-md-4">
            @Html.TextBoxFor(m => m.description, new { @class = "form-control", @placeholder = "EnterName", @requried = "required" })
        </div>

    </div>
    <div class="mb-3">
        <label for="Emailid" class="form-label">Ubicacion</label>
        <div class="col-md-4">
            @Html.TextBoxFor(m => m.address, new { @class = "form-control", @placeholder = "EnterName", @requried = "required" })
        </div>

    </div>
    <div class="mb-3">
        <label for="Emailid" class="form-label">Estatus</label>
        Activo:      @Html.RadioButtonFor(m => m.statut, "1")
        Desactivado: @Html.RadioButtonFor(m => m.statut, "0")
    </div>

    <button type="submit" value="" class="btn btn-primary">Save</button>
</form>
